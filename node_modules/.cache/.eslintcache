httpHook\";\nimport AuthContext from \"../Store/Auth/AuthContext\";\nimport { getAllLeads, getFilterColumns } from \"../Store/Leads/leads-actions\";\n\nconst LeadsModule = () => {\n    const ctx = useContext(AuthContext);\n\n    const { sendRequest: getLeads, isLoading, error: getLeadsError } = useHttp(getAllLeads, true);\n    const {sendRequest: fetchFilerColumns, isLoading: FilterLoading, error: filterError} = useHttp(getFilterColumns, true);\n    \n    const { filterColumns, leads, columns } = useSelector(state => state.leads);\n\n    useEffect(() => {\n       getLeads(ctx.orgId);\n       fetchFilerColumns(ctx.orgId);\n    },[ctx.orgId]);\n\n    if(getLeadsError.isValid || filterError.isValid) {\n        return <div style={{height:\"93vh\", backgroundColor:\"white\", color:\"red\", fontWeight:\"bold\", display:\"flex\", alignItems:\"center\", justifyContent:\"center\"}}>\n        { getLeadsError && filterError && <p>{getLeadsError.message}</p>}\n        <button>Please Re-Login</button>\n        </div>\n    }\n\n    return <div> \n                <ModuleActions path=\"add-lead\" module=\"Leads\"/>\n                <LeadsContainer filterCols={filterColumns} leadCols={ columns || [] } leads={ leads|| [] } isLoading={isLoading && FilterLoading} filterError={filterError} getLeadsError={getLeadsError}/>\n            </div>\n}\n\nexport default LeadsModule;","/Users/mukesh-8058/Documents/GitHub/CRM/src/Pages/Signin.js",["170"],"import styles from './SignIn.module.css';\nimport { Fragment, useContext } from 'react';\nimport { Link } from 'react-router-dom';\nimport LoadingPage from './loadingPage';\nimport useInput from '../Hooks/inputHook';\nimport AuthContext from '../Store/Auth/AuthContext';\nimport useHttp from '../Hooks/httpHook';\nimport { postSignIn } from '../library/authFns';\n\nconst isEmail = (value) => value.includes('@') && value.trim().length > 3;\nconst isEmpty = (value) => value.trim() !== \"\" && value.trim().length >= 3; \n\nconst SignIn = () => {\n    const ctx = useContext(AuthContext);  \n\n    const {value: email, \n            valueChangeHandler: emailChangehandler, \n            blurHandler: emailBlurHandler, \n            isValid: emailValid, \n            isTouched: emailTouched} = useInput(isEmail);\n    const {value: password, \n            valueChangeHandler: passwordChangehandler, \n            blurHandler: passwordBlurHandler, \n            isValid: passwordValid, \n            } = useInput(isEmpty);\n\n    const { isLoading, error, sendRequest } = useHttp(postSignIn, false);\n\n    const signInhandler = (e) => {\n        e.preventDefault();\n        if(emailValid && passwordValid) {\n            const signInObj = {\n                email,\n                password\n            }\n            sendRequest(signInObj, (data)=> {\n                ctx.signIn(data);\n            });\n        }\n    };\n\n    return <Fragment> \n                <div className={styles.signup}><button> <Link to=\"/signup\">SIGN UP</Link> </button></div>\n                <main className={styles.signinContainer}>\n                  { isLoading && <LoadingPage/>}\n                    <section className={styles.form}>\n                        <div className={styles[\"form-detail\"]}>\n                            <h1>Sign in</h1>\n                            <p>to access CRM</p>\n                        </div>\n                    <form onSubmit={signInhandler}>\n                        <div className={styles[\"form-control\"]}>\n                            <label htmlFor=\"email\">Email</label>\n                            <input className={styles.inp} type=\"text\" id=\"email\" onChange={emailChangehandler} onBlur={emailBlurHandler} accessKey='e'/>\n                            { !emailValid && emailTouched && <p className={styles.error}>Please enter a valid email</p> }\n                            { !error.isValid && error.value===\"email\" && <p className={styles.error}>{error.message}</p> }\n                        </div>\n                        <div className={styles[\"form-control\"]}>\n                            <label htmlFor=\"password\">Password</label>\n                            <input className={styles.inp} type=\"password\" id=\"password\" onChange={passwordChangehandler} onBlur={passwordBlurHandler}/>\n                            { !error.isValid && error.value===\"password\" && <p className={styles.error}>{error.message}</p> }\n                        </div>\n                        <button className={styles.submitBtn}>Sign In</button>\n                    </form>\n                </section>\n             </main>\n             </Fragment>\n}\n\nexport default SignIn;","/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Header/Header.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Leads/AddLead.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Leads/ViewLead.js",["171","172"],"import React, { useEffect, useState } from 'react';\nimport styles from './ViewLead.module.css';\nimport { useParams } from 'react-router';\nimport {  faBackward } from '@fortawesome/free-solid-svg-icons';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport useHttp from '../../Hooks/httpHook';\nimport TopBarLoading from '../../Pages/LoadingTopBar';\nimport { getSingleLeadRecord } from '../../Store/Leads/leads-actions';\nimport { useSelector } from 'react-redux';\nimport Notes from '../Notes/Notes';\n\nconst ViewLead = () => {\n\n    const params = useParams();\n    const {orgId, leadId} = params;\n    const [lead, setLead] = useState([]);\n    let displayRecord;\n    const notes = useSelector(state => state.leads.notes);\n    const {sendRequest : fetchLeadRecord , isLoading, error} = useHttp(getSingleLeadRecord, false);\n\n    useEffect(() => {\n        fetchLeadRecord({orgId, leadId}, (leadRecord) => {\n            setLead(leadRecord.record)\n        });\n        \n    }, []);\n    const navigateBackHandler = () => {\n\n    }\n\n    if(lead) {\n        displayRecord =  Object.keys(lead).map((leadR, index) => {\n                    return <div className={styles.field} key={index}> \n                                <div className={styles.label}>{ leadR }</div> \n                                <div className={styles.value}>{ lead[leadR] || \"-\" }</div> \n                            </div>\n                })\n       \n    }\n\nreturn <section className={styles.singleRecord}>\n            { isLoading && <TopBarLoading/>}\n            <section className={styles[\"singleRecord-actions\"]}>\n                <div className={styles.back}>\n                    <FontAwesomeIcon icon={faBackward} onClick={navigateBackHandler}/>\n                    <p>{ \"\" || lead.last_name}</p>\n                </div>\n                <div className={styles.action}>\n                    <button className={styles.convert}>Convert</button>\n                    <button className={styles.edit}>Edit</button>\n                    <button className={styles.clone}>Clone</button>\n                    <button className={styles.delete}>Delete</button>\n                </div>\n            </section>\n            <section className={styles.values}>\n                <section className={styles.rightPane}>\n                       <div className={styles.full}>\n                            <h1 className={styles.leadInfo}>Lead Information:</h1>\n                            <div className={styles.full}>  {displayRecord}</div>     \n                       </div>\n                       <div className={styles.notes}>\n                            <h1>Notes:</h1>\n                            {  <Notes notes={notes} orgId={orgId} RecId={leadId}  /> }\n                                              \n                       </div>\n                </section>\n            </section>\n</section>\n}\n\nexport default ViewLead;","/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Contacts/AddContact.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Store/Leads/leads-slice.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Contacts/ContactsContainer.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Pages/loadingPage.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Leads/LeadsActions.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Leads/LeadsContainer.js",["173"],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Store/Leads/leads-actions.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Hooks/httpHook.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Hooks/inputHook.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/library/authFns.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Hooks/useForm.js",["174"],"import { useEffect, useState, useCallback, useContext } from \"react\";\nimport useHttp from \"./httpHook\"\nimport AuthContext from \"../Store/Auth/AuthContext\";\n\nconst useForm = (requestFn) => {\n\n    const { isLoading, error, sendRequest } =  useHttp(requestFn, false);\n    const [formObj, setFormObj]= useState({});  \n\n    const ctx = useContext(AuthContext);\n\n    useEffect(() => {\n        sendRequest(ctx.orgId, (data)=> {\n            setFormObj(data);\n        })\n    },[]);\n\n  const inputHandler = useCallback((event, section) => {\n    \n     const { name, value } = event.target;\n     const newFormObj = {...formObj};\n    // code runs if address section is split into billing section\n     if(section === \"billing\") {\n         newFormObj.address.billing[name].value = value;\n         setFormObj({...newFormObj});\n     }\n     // code runs if address section is split into shipping section\n     if(section === \"shipping\"){\n        newFormObj.address.shipping[name].value = value;\n        setFormObj({...newFormObj});\n\n     } \n     // code runs if address section is not split and if the section is information section\n     if(section === \"address\" || section === \"information\") {\n        newFormObj[section][name].value = value;\n        setFormObj({...newFormObj});\n     }\n    \n  },[formObj])\n   \n    return {\n        inputHandler,\n        isLoading,\n        httpError: error,\n        address: formObj.address,\n        information: formObj.information\n    }\n}\n\nexport default useForm;","/Users/mukesh-8058/Documents/GitHub/CRM/src/Pages/LoadingTopBar.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/FormActions/FormActions.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Input/Input.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Notes/Notes.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Store/Contacts/contacts-actions.js",["175"],"import { contactsActions } from \"./contacts-slice\";\n\nconst url = \"http://localhost:8080\";\n\nexport const getContactsFields = async (orgId, token) => {\n        const response = await fetch(`${url}/contacts/org/${orgId}/contactsfields`,{\n            method:\"GET\",\n            headers: {\n                \"Content-Type\":\"application/json\",\n                \"Authorization\": `Bearer ${token}`\n            }\n        });\n        const responseData = await response.json();\n        if(!response.ok) {\n            let error = new Error(responseData.message || \"Error Occured!\")\n            error.isValid = responseData.isValid || false;\n            error.value = responseData.value || \"\";\n            throw(error);\n        }\n        return responseData;\n}","/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Filter/Filter.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Record/Record.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Notes/AddNote.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Store/Contacts/contacts-slice.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Store/Home/home-slice.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Store/Home/home-actions.js",[],"/Users/mukesh-8058/Documents/GitHub/CRM/src/Components/Dasboard/Dashboard.js",[],"/Users/mukesh-8058/Do