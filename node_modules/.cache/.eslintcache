 code runs if address section is split into shipping section\n     if(section === \"shipping\"){\n        newFormObj.address.shipping[name].value = value;\n        setFormObj({...newFormObj});\n\n     } \n     // code runs if address section is not split and if the section is information section\n     if(section === \"address\" || section === \"information\") {\n        newFormObj[section][name].value = value;\n        setFormObj({...newFormObj});\n     }\n    \n  },[formObj])\n   \n    return {\n        inputHandler,\n        isLoading,\n        httpError: error,\n        address: formObj.address,\n        information: formObj.information\n    }\n}\n\nexport default useForm;","/Users/mukesh-8058/Desktop/CRM/src/Components/FormActions/FormActions.js",[],"/Users/mukesh-8058/Desktop/CRM/src/Store/Contacts/contacts-actions.js",["137"],"import { contactsActions } from \"./contacts-slice\";\n\nconst url = \"http://localhost:8080\";\n\nexport const getContactsFields = async (orgId, token) => {\n        const response = await fetch(`${url}/org/${orgId}/contacts/contactsfields`,{\n            method:\"GET\",\n            headers: {\n                \"Content-Type\":\"application/json\",\n                \"Authorization\": `Bearer ${token}`\n            }\n        });\n        const responseData = await response.json();\n        if(!response.ok) {\n            let error = new Error(responseData.message || \"Error Occured!\")\n            error.isValid = responseData.isValid || false;\n            error.value = responseData.value || \"\";\n            throw(error);\n        }\n        return responseData;\n}","/Users/mukesh-8058/Desktop/CRM/src/Store/Contacts/contacts-slice.js",[],"/Users/mukesh-8058/Desktop/CRM/src/Components/Contacts/AddContact.js",[],"/Users/mukesh-8058/Desktop/CRM/src/Components/Leads/ViewLead.js",["138"],"import { useParams } from 'react-router';\nimport styles from './ViewLead.module.css';\n\nconst ViewLead = () => {\n\n    const params = useParams();\n